{
  "name": "ethoinfo-app",
  "version": "0.0.1",
  "description": "A mobile web app designed to enable the collection of flexible data structures in an offline environment. ",
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0 --colors --hot --inline --config webpack.dev.config.js --content-base ./public/",
    "prebuild": "npm install",
    "build": "mkdir -p dist && rimraf dist && webpack -p --verbose --colors --display-error-details --config webpack.production.config.js && cp public/index.html dist/index.html",
    "build-cordova": "npm run build && mkdir -p cordova/www/build && rimraf cordova/www/build && cp -R dist/build/ cordova/www/build/",
    "prebuild-cordova-ios": "npm run build-cordova",
    "build-cordova-ios": "cd cordova && cordova build ios",
    "prebuild-cordova-android": "npm run build-cordova",
    "build-cordova-android": "cd cordova && cordova build android",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sebastian Buys <sebastian@linebreakdesign.com>",
  "license": "?",
